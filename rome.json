{
	"formatter":{
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "tab",
		"indentSize": 2,
		"lineWidth": 80
	},
	"linter":{
		"enabled": true,
		"rules": {
			"suspicious":{
				"noExplicitAny": "off",
				"noArrayIndexKey": "off"
			},
			"security":{
				"noDangerouslySetInnerHtml": "off"
			}
			
		}
	}
}